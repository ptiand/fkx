<?xml version='1.0' encoding='utf-8'?>
<widget id="cn.fangkexing.app" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Fangkexing</name>
    <description>
        房客行app
    </description>
    <content src="https://www.fangkexing.cn/" />
    <icon src="res/icon/ICON_FKX_180x180.png" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-navigation href="http://*/*" />
    <allow-navigation href="https://*/*" />
    <feature name="StatusBar">
        <param name="ios-package" onload="true" value="CDVStatusBar" />
    </feature>
    <platform name="android">
        <allow-intent href="market:*" />
        <config-file parent="/*" target="AndroidManifest.xml">
            <uses-permission android:name="android.permission.INTERNET" />
            <uses-permission android:name="android.permission.ACCESS_LOCATION_EXTRA_COMMANDS" />
            <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
            <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
        </config-file>
        <edit-config file="AndroidManifest.xml" mode="overwrite" target="/manifest/application">
            <application android:label="房客行" />
        </edit-config>
        <preference name="AutoHideSplashScreen" value="true" />
        <preference name="SplashScreenDelay" value="3000" />
        <preference name="ShowSplashScreenSpinner" value="false" />
        <preference name="StatusBarBackgroundColor" value="#34BBBE" />
        <preference name="AppendUserAgent" value="fkxapp" />
        <icon density="ldpi" src="res/icon/ICON_FKX_40x40.png" />
        <icon density="mdpi" src="res/icon/ICON_FKX_58x58.png" />
        <icon density="hdpi" src="res/icon/ICON_FKX_80x80.png" />
        <icon density="xhdpi" src="res/icon/ICON_FKX_114x114.png" />
        <icon density="xxhdpi" src="res/icon/ICON_FKX_120x120.png" />
        <icon density="xxxhdpi" src="res/icon/ICON_FKX_180x180.png" />
        <splash density="land-ldpi" src="res/screen/StartPage_1024x768.png" />
        <splash density="land-mdpi" src="res/screen/StartPage_1024x768.png" />
        <splash density="land-hdpi" src="res/screen/StartPage_2048x1536.png" />
        <splash density="land-xhdpi" src="res/screen/StartPage_2208x1242.png" />
        <splash density="port-ldpi" src="res/screen/StartPage_320x480.png" />
        <splash density="port-mdpi" src="res/screen/StartPage_640x960.png" />
        <splash density="port-hdpi" src="res/screen/StartPage_768x1024.png" />
        <splash density="port-xhdpi" src="res/screen/StartPage_1242x2208.png" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <preference name="EnableViewportScale" value="true" />
        <meta content="width=device-width, initial-scale=1, user-scalable=no" name="viewport" />
        <preference name="BackupWebStorage" value="local" />
        <preference name="KeyboardDisplayRequiresUserAction" value="false" />
        <preference name="deployment-target" value="8.0" />
        <preference name="AppendUserAgent" value="fkxapp" />
        <icon height="180" src="res/icon/ICON_FKX_180x180.png" width="180" />
        <icon height="60" src="res/icon/ICON_FKX_60x60.png" width="60" />
        <icon height="120" src="res/icon/ICON_FKX_120x120.png" width="120" />
        <icon height="40" src="res/icon/ICON_FKX_40x40.png" width="40" />
        <icon height="80" src="res/icon/ICON_FKX_80x80.png" width="80" />
        <icon height="57" src="res/icon/ICON_FKX_57x57.png" width="57" />
        <icon height="114" src="res/icon/ICON_FKX_114x114.png" width="114" />
        <icon height="72" src="res/icon/ICON_FKX_72x72.png" width="72" />
        <icon height="144" src="res/icon/ICON_FKX_144x144.png" width="144" />
        <icon height="29" src="res/icon/ICON_FKX_29x29.png" width="29" />
        <icon height="58" src="res/icon/ICON_FKX_58x58.png" width="58" />
        <icon height="87" src="res/icon/ICON_FKX_87x87.png" width="87" />
        <icon height="76" src="res/icon/ICON_FKX_76x76.png" width="76" />
        <icon height="152" src="res/icon/ICON_FKX_152x152.png" width="152" />
        <icon height="48" src="res/icon/ICON_FKX_48x48.png" width="48" />
        <icon height="55" src="res/icon/ICON_FKX_55x55.png" width="55" />
        <icon height="172" src="res/icon/ICON_FKX_172x172.png" width="172" />
        <icon height="88" src="res/icon/ICON_FKX_88x88.png" width="88" />
        <icon height="196" src="res/icon/ICON_FKX_196x196.png" width="196" />
        <icon height="20" src="res/icon/ICON_FKX_20x20.png" width="20" />
        <icon height="50" src="res/icon/ICON_FKX_50x50.png" width="50" />
        <icon height="100" src="res/icon/ICON_FKX_100x100.png" width="100" />
        <icon height="167" src="res/icon/ICON_FKX_167x167.png" width="167" />
        <icon height="1024" src="res/icon/ICON_FKX_1024x1024.png" width="1024" />
        <splash height="480" src="res/screen/StartPage_320x480.png" width="320" />
        <splash height="960" src="res/screen/StartPage_640x960.png" width="640" />
        <splash height="1024" src="res/screen/StartPage_768x1024.png" width="768" />
        <splash height="2048" src="res/screen/StartPage_1536x2048.png" width="1536" />
        <splash height="768" src="res/screen/StartPage_1024x768.png" width="1024" />
        <splash height="1536" src="res/screen/StartPage_2048x1536.png" width="2048" />
        <splash height="1136" src="res/screen/StartPage_640x1136.png" width="640" />
        <splash height="1334" src="res/screen/StartPage_750x1334.png" width="750" />
        <splash height="2208" src="res/screen/StartPage_1242x2208.png" width="1242" />
        <splash height="1242" src="res/screen/StartPage_2208x1242.png" width="2208" />
        <preference name="StatusBarOverlaysWebView" value="false" />
        <preference name="StatusBarBackgroundColor" value="#34BBBE" />
        <preference name="StatusBarStyle" value="lightcontent" />
        <preference name="StatusBarDefaultScrollToTop" value="false" />
        <preference name="DisallowOverscroll" value="true" />
        <preference name="WebViewBounce" value="false" />
        <config-file parent="CFBundleDevelopmentRegion" target="*-Info.plist">
            <string>zh_CN</string>
        </config-file>
        <config-file parent="CFBundleDisplayName" target="*-Info.plist">
            <string>房客行</string>
        </config-file>
    </platform>
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
    <plugin name="cordova-plugin-splashscreen" spec="^5.0.2" />
    <plugin name="cordova-plugin-statusbar" spec="2.4.2" />
    <plugin name="cordova-plugin-remote-injection" spec="0.5.2" />
    <plugin name="cordova-plugin-geolocation" spec="4.0.1" />
    <engine name="browser" spec="^5.0.3" />
    <engine name="android" spec="7.0.0" />
    <engine name="ios" spec="4.5.4" />
</widget>
